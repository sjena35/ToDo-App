<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>WishtreeAssign</display-name>
  <servlet>
    <servlet-name>loginservlet1</servlet-name>
    <servlet-class>com.Login</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>loginservlet1</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>signupservlet1</servlet-name>
    <servlet-class>com.Signup</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>signupservlet1</servlet-name>
    <url-pattern>/signup</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>todoservlet1</servlet-name>
    <servlet-class>com.ToDoServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>todoservlet1</servlet-name>
    <url-pattern>/todoadd</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>completeservlet1</servlet-name>
    <servlet-class>com.CompleteServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>completeservlet1</servlet-name>
    <url-pattern>/complete</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>deactivateservlet1</servlet-name>
    <servlet-class>com.Deactivate</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>deactivateservlet1</servlet-name>
    <url-pattern>/activation</url-pattern>
  </servlet-mapping>
</web-app>